{% embed 'standardoutline.twig' %}
    {% block content %}
        <div class="lessons">
            <div class="lessonOptions">
                <input class="searchInput" placeholder="Search the site" type="search"/>
                <div class="rightAlignRow">
                    <a href="#">
                        <h1 class='listButton'>&#9776;</h1>
                    </a>
                    <a href="#">
                        <h1 class="gridButton">&#9638;</h1>
                    </a>
                </div>
            </div>
            <div class="list">
                {% for video in videoList %}
                    {% include 'twigElements/videoCard.twig' %}
                {% endfor %}
            </div>
        </div>
    {% endblock content %}
    {% block scriptImport %}
        {% include 'twigJSFiles/lessonsPage.twig' %}
    {% endblock scriptImport %}
{% endembed %}